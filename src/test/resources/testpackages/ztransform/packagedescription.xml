		<mvc:requiredRuntimeComponent>http://gis.geo.tu-dresden.de/movingcode/platformregistry/component/gdal-1.8</mvc:requiredRuntimeComponent>
		<mvc:requiredRuntimeComponent>http://gis.geo.tu-dresden.de/movingcode/platformregistry/component/gdal-1.8-python-2.7</mvc:requiredRuntimeComponent>
	</mvc:contractedPlatform>
