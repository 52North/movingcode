			<mvc:cc.attributionName>Matthias Mueller</mvc:cc.attributionName>
			<mvc:cc.attributionURL>http://tu-dresden.de/die_tu_dresden/fakultaeten/fakultaet_forst_geo_und_hydrowissenschaften/fachrichtung_geowissenschaften/gis/professur/team/mitarbeiter/mueller.m</mvc:cc.attributionURL>
		</mvc:creativeCommonsLicense>
	</mvc:exploitationRights>
	<mvc:workspace>
		<mvc:workspaceRoot>./ztransform</mvc:workspaceRoot>
		<mvc:executableLocation>./ztransform.py</mvc:executableLocation>
		<mvc:containerType>http://gis.geo.tu-dresden.de/movingcode/containerregistry/pythonscript-2.5</mvc:containerType>
		<mvc:executionParameters>
			<!-- Inputs -->
			<mvc:parameter>
				<mvc:prefixString />
				<mvc:suffixString />
				<mvc:separatorString>
				</mvc:separatorString>
				<mvc:positionID>3</mvc:positionID>
