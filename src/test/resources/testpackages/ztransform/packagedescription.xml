				<mvc:functionalOutputID />
			</mvc:parameter>
			<mvc:parameter>
				<mvc:prefixString />
				<mvc:suffixString />
				<mvc:separatorString />
				<mvc:positionID>2</mvc:positionID>
				<mvc:functionalInputID>REFERENCE_RASTER</mvc:functionalInputID>
				<mvc:functionalOutputID />
			</mvc:parameter>
			<!-- Outputs -->
			<mvc:parameter>
				<mvc:prefixString />
				<mvc:suffixString />
				<mvc:separatorString />
				<mvc:positionID>1</mvc:positionID>
				<mvc:functionalInputID />
				<mvc:functionalOutputID>ZTRANSFORM</mvc:functionalOutputID>
			</mvc:parameter>
		</mvc:executionParameters>
	</mvc:workspace>
