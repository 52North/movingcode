		<mc:requiredRuntimeComponent>http://gis.geo.tu-dresden.de/movingcode/platformregistry/component/gdal-1.8-python-2.7</mc:requiredRuntimeComponent>
	</mc:platform>
	<mc:infrastructure>
		<mc:occi.compute.cores>1</mc:occi.compute.cores>
