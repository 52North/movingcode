		<pc:availablePlatform>http://gis.geo.tu-dresden.de/movingcode/platformregistry/platform/arcgis-10.0-analysis</pc:availablePlatform>
		<pc:availablePlatform>http://gis.geo.tu-dresden.de/movingcode/platformregistry/platform/java-1.6</pc:availablePlatform>
	</pc:defaults>
</pc:processors>
