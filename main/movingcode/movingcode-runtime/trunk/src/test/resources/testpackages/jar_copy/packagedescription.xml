<?xml version="1.0" encoding="UTF-8"?>
<!-- Description for echo process Author: Matthias Mueller, TU 
	Dresden Website: http://tu-dresden.de/fgh/geo/gis -->
<mvc:packageDescription xmlns:mvc="http://gis.geo.tu-dresden.de/movingcode/1.0.0"
	xmlns:ows="http://www.opengis.net/ows/1.1" xmlns:wps="http://www.opengis.net/wps/1.0.0"
	xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xml="http://www.w3.org/XML/1998/namespace"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://gis.geo.tu-dresden.de/movingcode/1.0.0
						http://gis.geo.tu-dresden.de/movingcode/1.0.0/descriptions/MovingCodePackage.xsd">
	<mvc:contractedFunctionality>
		<mvc:wpsProcessDescription
			wps:processVersion="1">
			<ows:Identifier>de.tu-dresden.geo.gis.algorithms.test.echo</ows:Identifier>
			<ows:Title>Echo process</ows:Title>
			<ows:Abstract>Returns the input</ows:Abstract>
			<DataInputs>
				<Input minOccurs="1" maxOccurs="1">
					<ows:Identifier>CALL</ows:Identifier>
					<ows:Title>The Echo Call</ows:Title>
					<ows:Abstract>Some arbitrary input</ows:Abstract>
					<ComplexData>
						<Default>
							<Format>
								<MimeType>application/geotiff</MimeType>
								<Schema></Schema>
							</Format>
						</Default>
						<Supported>
							<Format>
								<MimeType>application/geotiff</MimeType>
								<Schema></Schema>
							</Format>
						</Supported>
					</ComplexData>
				</Input>
			</DataInputs>
			<ProcessOutputs>
				<Output>
					<ows:Identifier>RESPONSE</ows:Identifier>
					<ows:Title>Echo Response</ows:Title>
					<ows:Abstract>The same arbitrary response</ows:Abstract>
					<ComplexOutput>
						<Default>
							<Format>
								<MimeType>application/geotiff</MimeType>
								<Schema></Schema>
							</Format>
						</Default>
						<Supported>
							<Format>
								<MimeType>application/geotiff</MimeType>
								<Schema></Schema>
							</Format>
						</Supported>
					</ComplexOutput>
				</Output>
			</ProcessOutputs>
		</mvc:wpsProcessDescription>
	</mvc:contractedFunctionality>
	<mvc:contractedPlatform mvc:platformId="http://gis.geo.tu-dresden.de/movingcode/platformregistry/platform/java-1.6"/>
	<mvc:contractedInfrastructure>
		<mvc:occi.compute.memory>0.1</mvc:occi.compute.memory>
		<mvc:occi.storage.size>0.01</mvc:occi.storage.size>
	</mvc:contractedInfrastructure>
	<mvc:exploitationRights>
		<mvc:creativeCommonsLicense>
			<mvc:cc.license>http://creativecommons.org/licenses/by-sa/3.0/</mvc:cc.license>
			<mvc:dct.title>de.tu-dresden.geo.gis.algorithms.test.echo</mvc:dct.title>
			<mvc:cc.attributionName>Matthias Mueller</mvc:cc.attributionName>
			<mvc:cc.attributionURL>http://tu-dresden.de/die_tu_dresden/fakultaeten/fakultaet_forst_geo_und_hydrowissenschaften/fachrichtung_geowissenschaften/gis/professur/team/mitarbeiter/mueller.m</mvc:cc.attributionURL>
		</mvc:creativeCommonsLicense>
	</mvc:exploitationRights>
	<mvc:workspace>
		<mvc:workspaceRoot>./jar_copy</mvc:workspaceRoot>
		<mvc:executableLocation>./jarcopy.jar</mvc:executableLocation>
		<mvc:containerType>http://gis.geo.tu-dresden.de/movingcode/containerregistry/java-jar</mvc:containerType>
		<mvc:executionParameters>
			<!-- Inputs -->
			<mvc:parameter>
				<mvc:positionID>1</mvc:positionID>
				<mvc:functionalInputID>CALL</mvc:functionalInputID>
			</mvc:parameter>
			<!-- Outputs -->
			<mvc:parameter>
				<mvc:positionID>2</mvc:positionID>
				<mvc:functionalOutputID>RESPONSE</mvc:functionalOutputID>
			</mvc:parameter>
		</mvc:executionParameters>
	</mvc:workspace>
</mvc:packageDescription>
